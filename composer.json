{
    "name": "railken/laravel-ore-starter",
    "description": "The starter pack for lara-ore packages",
    "keywords": ["framework", "laravel", "lara-ore"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^6.0",
        "laravel/tinker": "^1.0",
        "railken/lara-ore-account": "dev-master",
        "railken/lara-ore-auth": "dev-master",
        "railken/lara-ore-notification": "dev-master",
        "railken/lara-ore-user": "dev-master",
        "railken/lara-ore-config": "dev-master",
        "railken/lara-ore-disk": "dev-master",
        "railken/lara-ore-file": "dev-master",
        "railken/lara-ore-repository": "dev-master",
        "railken/lara-ore-data-builder": "dev-master",
        "railken/lara-ore-template": "dev-master",
        "railken/lara-ore-exporter": "dev-master",
        "railken/lara-ore-file-generator": "dev-master",
        "railken/lara-ore-email-sender": "dev-master"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpstan/phpstan": "^0.10.2",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
